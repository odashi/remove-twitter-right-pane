{
    "name": "Remove Twitter Right Pane",
    "version": "0.1",
    "description": "Removes right-hand pane in the Twitter website.",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://twitter.com/*"],
            "js": ["jquery-3.5.1.min.js", "content.js"]
        }
    ]
}
